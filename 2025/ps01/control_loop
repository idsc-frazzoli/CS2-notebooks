digraph {
	rankdir=LR
	N [label="Noise
(Intensity: 0.50, Freq: 50Hz)"]
	F [label="Anti-Aliasing Filter
(Cutoff: 200Hz)"]
	S [label="Sampler
(Sampling Freq: 1000Hz)"]
	P [label="CT Plant Model"]
	C [label="DT PID Controller"]
	ZOH [label="Zero-Order Hold"]
	N -> F
	F -> S
	S -> C
	C -> ZOH
	ZOH -> P
	P -> S [label=Feedback]
}
